    "@eggjs/tegg-types": "^3.72.0"
  },
  "devDependencies": {
    "@eggjs/controller-decorator": "^3.72.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.4",
    "cross-env": "^7.0.3",
